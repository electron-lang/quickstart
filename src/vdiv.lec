import $R from "electron_lec/src/cells"

/// A voltage divider.
export module VoltageDivider {
  @left analog vin
  @right analog vout
  @bottom analog gnd

  @rotate(90)
  @value("10K")
  @footprint("vdiv:R0402")
  @bom("Yageo", "RC0402FR-0710KL")
  cell r1 = $R(10k) {A=vin, B=vout}

  @rotate(90)
  @value("10K")
  @footprint("vdiv:R0402")
  @bom("Yageo", "RC0402FR-0710KL")
  cell r2 = $R(10k) {A=vout, B=gnd}
}